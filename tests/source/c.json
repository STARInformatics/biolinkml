{
  "@version": 1.1,
  "@vocab": "http://janeirodigital.github.io/nhs-care-plan/flat-FHIR.ttl#",
  "xsd": "http://www.w3.org/2001/XMLSchema#" ,
  "fhir": "http://hl7.org/fhir/",
  "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",

  "id": { "@id": "fhir:Resource.id" } ,
  "resourceType": { "@id": "rdf:type" , "@type": "@id" } ,
  "extension": {
    "@id": "fhir:DomainResource.extension",
    "@context": {
      "url": { "@id": "fhir:Extension.url" },
      "valueString": "fhir:Extension.valueString",
      "valueDateTime": { "@id": "fhir:Extension.valueDateTime", "@type": "xsd:date" },
      "valueCode": {
        "@id": "fhir:Extension.valueCode",
        "@context": {
          "code": "fhir:Coding.code"
        }
      }
    }
  } ,

  "code": {
    "@id": "fhir:Observation.code",
    "@context": {
      "coding": {
        "@id": "fhir:CodeableConcept.coding",
        "@context": {
          "code": "fhir:Coding.code",
          "display": "fhir:Coding.display",
          "system": {
            "@id": "fhir:Coding.system",
            "@type": "@id"
          }
        }
      }
    }
  } ,
  "status": "fhir:Observation.status",
  "valueCode": "fhir:Observation.valueCode",
  "valueString": "fhir:Observation.valueString",
  "valueDateTime": { "@id": "fhir:Observation.valueDateTime", "@type": "xsd:date" } ,
  "valuePeriod": {
    "@id": "fhir:Observation.valuePeriod",
    "@context": {
      "start": { "@id": "fhir:Period.start", "@type": "xsd:dateTime" } ,
      "end": { "@id": "fhir:Period.end", "@type": "xsd:dateTime" }
    }
  } ,
  "valueCodeableConcept": {
    "@id": "fhir:Observation.valueCodeableConcept",
    "@context": {
      "coding": {
        "@id": "fhir:CodeableConcept.coding",
        "@context": {
          "code": "fhir:Coding.code",
          "display": "fhir:Coding.display",
          "system": {
            "@id": "fhir:Coding.system",
            "@type": "@id"
          } ,
          "extension": {
            "@id": "fhir:Element.extension",
            "@context": {
              "url": { "@id": "fhir:Extension.url" },
              "valueId": "fhir:Extension.valueId"
            }
          }
        }
      }
    }
  } ,
  "valueBoolean": "fhir:Observation.valueBoolean",
  "note": {
    "@id": "fhir:Observation.note",
    "@context": {
      "text": {
        "@id": "fhir:Annotation.text",
        "@context": {
          "value": "fhir:value"
        }
      }
    }
  } ,
  "issued": { "@id": "fhir:Observation.issued", "@type": "xsd:dateTime" }
}
